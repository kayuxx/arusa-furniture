#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/kayuxx/Projects/my-turborepo/node_modules/.pnpm/vite@7.1.1_@types+node@22.17.0_jiti@2.5.1_lightningcss@1.30.1_sass@1.77.4_tsx@4.20.3_yaml@2.8.1/node_modules/vite/bin/node_modules:/home/kayuxx/Projects/my-turborepo/node_modules/.pnpm/vite@7.1.1_@types+node@22.17.0_jiti@2.5.1_lightningcss@1.30.1_sass@1.77.4_tsx@4.20.3_yaml@2.8.1/node_modules/vite/node_modules:/home/kayuxx/Projects/my-turborepo/node_modules/.pnpm/vite@7.1.1_@types+node@22.17.0_jiti@2.5.1_lightningcss@1.30.1_sass@1.77.4_tsx@4.20.3_yaml@2.8.1/node_modules:/home/kayuxx/Projects/my-turborepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/kayuxx/Projects/my-turborepo/node_modules/.pnpm/vite@7.1.1_@types+node@22.17.0_jiti@2.5.1_lightningcss@1.30.1_sass@1.77.4_tsx@4.20.3_yaml@2.8.1/node_modules/vite/bin/node_modules:/home/kayuxx/Projects/my-turborepo/node_modules/.pnpm/vite@7.1.1_@types+node@22.17.0_jiti@2.5.1_lightningcss@1.30.1_sass@1.77.4_tsx@4.20.3_yaml@2.8.1/node_modules/vite/node_modules:/home/kayuxx/Projects/my-turborepo/node_modules/.pnpm/vite@7.1.1_@types+node@22.17.0_jiti@2.5.1_lightningcss@1.30.1_sass@1.77.4_tsx@4.20.3_yaml@2.8.1/node_modules:/home/kayuxx/Projects/my-turborepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vite@7.1.1_@types+node@22.17.0_jiti@2.5.1_lightningcss@1.30.1_sass@1.77.4_tsx@4.20.3_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vite@7.1.1_@types+node@22.17.0_jiti@2.5.1_lightningcss@1.30.1_sass@1.77.4_tsx@4.20.3_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
fi
