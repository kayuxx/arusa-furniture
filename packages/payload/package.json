{
  "name": "@repo/payload",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "exports": {
    "./*": "./src/*",
    "./access/*": "./src/access/*",
    "./collections/*": "./src/collections/*",
    "./components/*": "./src/components/*",
    "./fields/*": "./src/fields/*",
    "./plugins/*": "./src/plugins/*",
    "./payload-types": "./src/payload.types.ts",
    "./payload-config": "./src/payload.config.ts"
  },
  "scripts": {
    "clean": "rm -rf node_modules .pnpm-lock.yaml",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ./.prettierignore",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "lint": "eslint .",
    "reinstall": "pnpm clean && pnpm i",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.51.0",
    "@payloadcms/next": "3.51.0",
    "@payloadcms/payload-cloud": "3.51.0",
    "@payloadcms/plugin-nested-docs": "^3.51.0",
    "@payloadcms/plugin-seo": "^3.51.0",
    "@payloadcms/richtext-lexical": "3.51.0",
    "@payloadcms/storage-vercel-blob": "^3.51.0",
    "@payloadcms/ui": "3.51.0",
    "@repo/ui": "workspace:*",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "graphql": "^16.8.1",
    "i18n-iso-countries": "^7.14.0",
    "next": "^15.4.2",
    "payload": "3.51.0",
    "react": "^19.1.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.1.0",
    "sharp": "0.34.3",
    "use-debounce": "^10.0.5"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.4.2",
    "@playwright/test": "1.54.1",
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/postcss": "^4.1.5",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.31.0",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "postcss": "^8.5.3",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.1.5",
    "typescript": "5.8.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  }
}
